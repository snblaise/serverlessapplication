{
  "name": "lambda-production-readiness",
  "version": "1.0.0",
  "description": "Production-ready AWS Lambda with comprehensive CI/CD pipeline",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src/ --ext .js,.ts --fix",
    "lint:check": "eslint src/ --ext .js,.ts",
    "test": "jest --coverage --testResultsProcessor=jest-sonar-reporter",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --watchAll=false --testResultsProcessor=jest-sonar-reporter",
    "build": "npm run lint:check && npm run test:ci",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npm audit --audit-level=high --json",
    "validate": "npm run lint:check && npm run test:ci && npm run security:check"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.17.0",
    "@aws-lambda-powertools/metrics": "^1.17.0",
    "@aws-lambda-powertools/tracer": "^1.17.0",
    "aws-sdk": "^2.1490.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.126",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
  },

  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/lambda-production-readiness.git"
  },
  "keywords": [
    "aws",
    "lambda",
    "serverless",
    "production-ready",
    "ci-cd",
    "security"
  ],
  "author": "Your Organization",
  "license": "MIT"
}